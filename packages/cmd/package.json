    "command line",
    "cmd"
  ],
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/Hitotsubashi/sagaroute/issues"
  },
  "homepage": "https://github.com/Hitotsubashi/sagaroute/tree/main/packages/cmd",
  "license": "MIT",
  "main": "lib/index.js",
